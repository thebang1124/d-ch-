            },
            {
                ede: "Truh",
                meanings: [{ type: "đgt", vietnamese: "(Đã) tới một địa điểm nào đó", examples: ["Kão truh kơ sang"] }]
            }
        ];

        // The processDictionaryData function is no longer needed as data is pre-structured.

        // Get DOM elements
        const searchInput = document.getElementById('searchInput');
        const resultsDisplay = document.getElementById('resultsDisplay');
        const clearButton = document.getElementById('clearButton');

        // Function to display search results
        function displayResults(results) {
            resultsDisplay.innerHTML = ''; // Clear previous results

            if (results.length === 0) {
                resultsDisplay.innerHTML = '<p class="text-gray-500">Không tìm thấy từ nào phù hợp.</p>';
                return;
            }

            results.forEach(entry => {
                const resultItem = document.createElement('div');
                resultItem.className = 'result-item';

                let meaningsHtml = '';
                entry.meanings.forEach(meaning => {
                    let examplesHtml = '';
                    if (meaning.examples && meaning.examples.length > 0) {
                        examplesHtml = `
                            <div class="examples">
                                <span class="font-medium">Ví dụ:</span>
                                <ul class="list-disc list-inside ml-4">
                                    ${meaning.examples.map(ex => `<li>${ex}</li>`).join('')}
                                </ul>
                            </div>
                        `;
                    }
                    meaningsHtml += `
                        <p><span class="font-medium">Nghĩa (${meaning.type}):</span> ${meaning.vietnamese}</p>
                        ${examplesHtml}
                    `;
                });

                resultItem.innerHTML = `
                    <h3>${entry.ede}</h3>
                    ${meaningsHtml}
                `;
                resultsDisplay.appendChild(resultItem);
            });
        }

        // Function to perform search
        function performSearch() {
            const searchTerm = searchInput.value.toLowerCase().trim();
            if (searchTerm === '') {
                resultsDisplay.innerHTML = '<p class="text-gray-500">Vui lòng nhập từ để tìm kiếm.</p>';
                return;
            }

            const filteredResults = dictionaryData.filter(entry => {
                // Check if the Êđê word itself matches
                if (entry.ede.toLowerCase().includes(searchTerm)) {
                    return true;
                }
                // Check if any of the Vietnamese meanings or examples match
                return entry.meanings.some(meaning =>
                    meaning.vietnamese.toLowerCase().includes(searchTerm) ||
                    meaning.examples.some(example => example.toLowerCase().includes(searchTerm))
                );
            });
            displayResults(filteredResults);
        }

        // Event Listeners
        searchInput.addEventListener('input', performSearch);
        clearButton.addEventListener('click', () => {
            searchInput.value = '';
            resultsDisplay.innerHTML = '<p class="text-gray-500">Vui lòng nhập từ để tìm kiếm.</p>';
        });

        // Initial display
        displayResults([]); // Show initial message
    </script>
</body>
</html>
